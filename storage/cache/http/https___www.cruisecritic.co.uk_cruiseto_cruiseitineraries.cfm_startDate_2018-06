var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];
(function() {
    var gads = document.createElement("script");
    gads.async = true;
    gads.type = "text/javascript";

    var useSSL = "https:" == document.location.protocol;
    gads.src = (useSSL ? "https:" : "http:") + "//www.googletagservices.com/tag/js/gpt.js";

    var node = document.getElementsByTagName("script")[0];
    node.parentNode.insertBefore(gads, node);
})();
</script>
<script>
var googletagHelper = function () {
    this.ads = {};
    this.defineAd = function (id) {
        this.ads[id] = {
            'empty': []
        };
    },
    this.defineAds = function (ids) {
        for (var i in ids) {
            this.defineAd(ids[i]);
        }
    },
    this.displayAd = function (id) {
        googletag.cmd.push(function() {
            googletag.display(id);
        });
    },
    this.displayAdConditionally = function (id, idEmpty) {
        this.ads[idEmpty].empty.push(id);
    },
    this.slotRenderEnded = function (event) {
        var id = event.slot.getSlotElementId();
        var adIds = this.ads[id].empty;
        if (event.isEmpty && adIds.length) {
            for (var i in adIds) {
                this.displayAd(adIds[i]);
            }
        }
    }
    return this;
}();
</script>
